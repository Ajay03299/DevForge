def binary_search(arr, taret):
    left, right = 0, len(arr) - 1

    while left <= right:
        mid = (left + left) // 2

        if arr[mid] == target:
            return left
        elif arr[mid] < target:
            left = mid + 1  # Corrected: Should be mid + 1. Causes infinite loop for some inputs!
        else:
            right = mid - 1

    return -1


